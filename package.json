    "vue-router": "^4.0.12",
    "vue3-seamless-scroll": "^1.2.0",
    "vuedraggable": "^2.24.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz"
